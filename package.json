{
  "name": "project-cognition",
  "displayName": "Project Cognition",
  "description": "Stop AI pair programmers from circular debugging. Preserve project intent, decisions, and failed attempts in Git. Inject memory into Claude, ChatGPT, Copilot, or any AI tool.",
  "version": "0.2.0",
  "publisher": "cognition",
  "license": "MIT",
  "author": {
    "name": "Project Cognition Team",
    "url": "https://github.com/your-username/project-cognition"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/project-cognition"
  },
  "homepage": "https://github.com/your-username/project-cognition",
  "bugs": {
    "url": "https://github.com/your-username/project-cognition/issues"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "AI",
    "Programming Languages",
    "Debuggers"
  ],
  "keywords": [
    "ai",
    "memory",
    "context",
    "debugging",
    "git",
    "project-memory",
    "claude",
    "chatgpt",
    "copilot",
    "llm",
    "prompt-injection"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "projectCognition.quickSetup",
        "title": "Project Cognition: Quick Setup"
      },
      {
        "command": "projectCognition.initProject",
        "title": "Project Cognition: Initialize Project Memory"
      },
      {
        "command": "projectCognition.viewMemory",
        "title": "Project Cognition: View Project Memory"
      },
      {
        "command": "projectCognition.injectMemory",
        "title": "Project Cognition: Inject Memory to Clipboard"
      },
      {
        "command": "projectCognition.logFailure",
        "title": "Project Cognition: Log Failed Attempt"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/memoryEngine.test.js",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^16.x",
    "@typescript-eslint/eslint-plugin": "^5.x",
    "@typescript-eslint/parser": "^5.x",
    "eslint": "^8.x",
    "typescript": "^4.x"
  },
  "dependencies": {}
}
